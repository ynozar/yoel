{
  "name": "yoel",
  "version": "1.0.2",
  "description": "Yoel Nozar's digital business card. Run `npx yoel` to see it!",
  "homepage": "https://github.com/ynozar/yoel#readme",
  "bugs": {
    "url": "https://github.com/ynozar/yoel/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ynozar/yoel.git"
  },
  "license": "ISC",
  "author": "Yoel Nozar",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
